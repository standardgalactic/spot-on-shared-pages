                            data-sizes="auto"
                            data-srcset="https://img.assets-d.propublica.org/v5/images/20230218-Arterial-Motives-impact-folo.jpg?crop=focalpoint&amp;fit=crop&amp;fp-x=0.5&amp;fp-y=0.5&amp;h=75&amp;q=70&amp;w=75&amp;s=1c5bee80f7a97700fdb598dd83a13eaa 75w, https://img.assets-d.propublica.org/v5/images/20230218-Arterial-Motives-impact-folo.jpg?crop=focalpoint&amp;fit=crop&amp;fp-x=0.5&amp;fp-y=0.5&amp;h=150&amp;q=70&amp;w=150&amp;s=a2a7230ac0d0f9a21a033c302f6351b4 150w, https://img.assets-d.propublica.org/v5/images/20230218-Arterial-Motives-impact-folo.jpg?crop=focalpoint&amp;fit=crop&amp;fp-x=0.5&amp;fp-y=0.5&amp;h=400&amp;q=70&amp;w=400&amp;s=bbeadec4307f69658e8b374d5db6b4de 400w"
