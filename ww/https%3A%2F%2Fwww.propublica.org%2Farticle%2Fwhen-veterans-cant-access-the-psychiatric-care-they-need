    <script type="text/javascript">
    // horizontal scrollers

      const hScrollerList = document.querySelectorAll(".hs-scroller");
      const clampNumber = (min, num, max) =>
        Math.max(Math.min(num, Math.max(min, max)), Math.min(min, max));

      var clientHeight = 0;
      var clientWidth = 0;

      function hScrollerUpdateHeights() {
        hScrollerList.forEach((element) => {
          if (!clientHeight) {
            clientHeight = document.documentElement.clientHeight;
            clientWidth = document.documentElement.clientWidth;
          }
          element.setAttribute(
            "style",
            "height: " +
              element.querySelector(".hs-scroller__items").scrollWidth +
              "px"
          );
        });
      }

      function hScrollerUpdateScroll() {
        hScrollerList.forEach((element) => {
          const hScrollerBounds = element.getBoundingClientRect();

          const hScrollRatio =
            hScrollerBounds.top /
            (hScrollerBounds.height -
              clientHeight); /* Returns a number between -1 and 0, representing progress */
          const hScrollTotalAmount = element.querySelector(
            ".hs-scroller__items"
          ).scrollWidth;

          const translateAmount = Math.round(
            clampNumber(-1, hScrollRatio, 0) * (hScrollTotalAmount - clientWidth)
          );

         
          element
            .querySelector(".hs-scroller__items")
            .setAttribute(
              "style",
              "transform: translateX(" + translateAmount + "px);"
            );
       
        });
      }

      ["DOMContentLoaded", "resize"].forEach(function (e) {
        window.addEventListener(e, function () {
          clientHeight = document.documentElement.clientHeight;
          clientWidth = document.documentElement.clientWidth;
          hScrollerUpdateHeights();
        });
      });

      ["scroll", "resize"].forEach(function (e) {
        window.addEventListener(e, function () {
          hScrollerUpdateScroll();
        });
      });
    </script>
</div>
    

    
    <div class="article-body" data-pp-location="article body" >

        
