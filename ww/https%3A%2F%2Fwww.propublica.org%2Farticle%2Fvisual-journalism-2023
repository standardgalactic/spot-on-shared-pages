/*  to set a custom background color for dark mode */

  html.force-dark-mode .bg-white {
    --color-bg: rgba(33,33,33,1);
  } 
    
  @media (prefers-color-scheme: dark) {
    html:not(.force-light-mode) .bg-white {
      --color-bg: rgba(33,33,33,1);
    }
  }

.bb-image, .bb-video-self-hosted, .bb-image-set{
padding-bottom:var(--spacing2); 
}

/* horizontal scrollers */
.hs-scroller__wrapper {
    height: 100vh;
    display: flex;
    flex-direction: column;
    gap: 1rem;
    justify-content: center;
    align-items: left;
    position: sticky;
    top: 0;
    overflow-x: hidden;
    width: 100vw;
}

.hs-scroller__items {
    display: flex;
    align-items: center;
    gap: 2rem;
    transition: transform 0.01s;
}

.hs-scroller__item img {
    width: auto;
    height: 70vmin;
}

.hs-scroller__item:first-of-type figure {
    margin-left: var(--spacing1);
}

.hs-scroller__item:last-of-type figure {
    margin-right: var(--spacing1);
}

.article-body>.full .lead-in,
.article-body>.full .attribution {
    margin-left: var(--spacing1);
    margin-right: var(--spacing1);
}

@media screen and (min-width: 48em) {
    .hs-scroller__item:first-of-type figure {
        margin-left: var(--spacing3);
    }

    .hs-scroller__item:last-of-type figure {
        margin-right: var(--spacing3);
    }

    .article-body>.full .lead-in,
    .article-body>.full .attribution {
        margin-left: var(--spacing3);
        margin-right: var(--spacing3);
    }
}

/* center attribution */

.attribution {
text-align: center;
margin-top: 1rem;
}

</style>


