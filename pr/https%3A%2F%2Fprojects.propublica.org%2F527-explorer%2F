<script>
    function toggleAdvancd(){
        document.getElementById('advanced-explanation').classList.toggle('hidden');

        var more_info = document.getElementById('more_info');
        more_info.innerHTML = more_info.innerHTML.includes('+') ? '<a>Advanced search -</a>' : '<a>Advanced search +</a>';

    }
</script>
