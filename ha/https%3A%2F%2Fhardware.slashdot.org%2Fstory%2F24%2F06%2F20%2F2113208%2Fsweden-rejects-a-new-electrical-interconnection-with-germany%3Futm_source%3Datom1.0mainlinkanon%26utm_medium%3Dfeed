function closeAnnouncement() {
	$('#announcement').fadeOut(300);
	$.cookie('hide_sitenotice_36', 'true', { path: '/', domain: 'slashdot.org', expires: 1 });
}
</script>
				<article id="firehose-174216281" data-fhid="174216281" data-fhtype="story" class="fhitem fhitem-story article usermode thumbs grid_24">
