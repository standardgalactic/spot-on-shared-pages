		<a href="//idle.slashdot.org/index2.pl?fhfilter=power" onclick="return addfhfilter('power');">
			
				<img src="//a.fsdn.com/sd/topics/power_64.png" width="64" height="64" alt="Power" title="Power">
			
		</a>
		
	
		
		

		
		<a href="//idle.slashdot.org/index2.pl?fhfilter=idle" onclick="return addfhfilter('idle');">
			
				<img src="//a.fsdn.com/sd/topics/idle_64.png" width="64" height="64" alt="Idle" title="Idle">
			
