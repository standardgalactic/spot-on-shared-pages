        e("https://pixel.condenastdigital.com/sparrow.min.js", function() {
          if (window.SparrowConfigV2) {
            window.sparrow = new window.Sparrow(window.SparrowConfigV2);
            new DQ();
          }
        })
      })
    }
  })();
</script>
<!-- Sparrow end -->
